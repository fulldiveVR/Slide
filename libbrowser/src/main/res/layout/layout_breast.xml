<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_height="match_parent"
    android:layout_width="match_parent">

    <LinearLayout
        android:gravity="center_horizontal|end"
        android:id="@+id/informaticaLayout"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="vertical"
        app:layout_constraintBottom_toTopOf="@id/chenilleView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:gravity="top|center_vertical"
            android:id="@+id/gentlewomenPinpointLayout"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:paddingBottom="14dp"
            android:paddingEnd="@dimen/florence_hallucinogen_padding"
            android:paddingStart="@dimen/ovid_extracurricular_padding">

            <CheckBox
                android:background="@color/nepotistic_televise"
                android:id="@+id/lineupView"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:text="@string/proclivity_bestow_lability"
                android:textColor="@color/hippie_witt_doormen" />

            <Button
                android:id="@+id/astraddleMenialView"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:text="@string/stillbirth_lawman_squirehood"
                android:textColor="@color/confederate_preposterous" />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/indyLayout"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/madame_roister_padding"
                android:layout_width="wrap_content">

                <EditText
                    android:id="@+id/showplaceKiowaView"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:text="@string/sudden_omega_roadway_panacea"
                    android:textColor="@color/shell_salutary"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />
            </androidx.constraintlayout.widget.ConstraintLayout>

            <EditText
                android:background="@color/bimodal_contract_wince"
                android:id="@+id/liquefyDavitView"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:text="@string/someday_spinnaker_prophesy"
                android:textColor="#A34529DD" />

            <Button
                android:background="#91B8C0"
                android:id="@+id/slumpMetierView"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:paddingBottom="@dimen/offshore_stenographer"
                android:paddingEnd="@dimen/reedy_fuselage_margin"
                android:paddingTop="@dimen/coin_hipster"
                android:text="@string/acrobatic_remediable_hopscotch"
                android:textColor="@color/mississippian_ingratiate_helmsman"
                android:textSize="@dimen/obligatory_propagandist_textsize" />
        </LinearLayout>

        <CheckBox
            android:background="@color/fiberboard_oakland_attendee"
            android:id="@+id/ravineInvolveView"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/coin_hipster"
            android:layout_width="match_parent"
            android:text="@string/diphthong_phylogenetic_icing"
            android:textColor="@color/copperas_parquet"
            android:textSize="@dimen/geminid_mossy_textsize" />
    </LinearLayout>

    <TextView
        android:background="#B54DCAA9"
        android:id="@+id/chenilleView"
        android:layout_height="wrap_content"
        android:layout_width="0dp"
        android:padding="@dimen/coin_hipster"
        android:text="@string/roadster_biology_handpicked_tine"
        android:textColor="@color/vermeil_pinehurst_manatee"
        android:textSize="14sp"
        app:layout_constraintBottom_toTopOf="@id/wildcatView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/informaticaLayout" />

    <Button
        android:background="@color/vermeil_pinehurst_manatee"
        android:id="@+id/wildcatView"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:text="@string/errancy_creole_harebrained_theseus"
        android:textColor="@color/icon_detent_buses"
        app:layout_constraintBottom_toTopOf="@id/yugoslaviaBlinkView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/chenilleView" />

    <EditText
        android:background="@color/fiberboard_oakland_attendee"
        android:id="@+id/yugoslaviaBlinkView"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/buzz_spec_size"
        android:layout_width="wrap_content"
        android:padding="@dimen/renegotiable_fudge_padding"
        android:text="@string/share_bracelet_novosibirsk"
        android:textSize="@dimen/frenzy_inductance_textsize"
        app:layout_constraintBottom_toTopOf="@id/deserveHeadroomLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/wildcatView" />

    <LinearLayout
        android:gravity="center_horizontal"
        android:id="@+id/deserveHeadroomLayout"
        android:layout_height="wrap_content"
        android:layout_width="0dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toTopOf="@id/antiquaryView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/yugoslaviaBlinkView">

        <CheckBox
            android:background="@color/wingman_vida"
            android:id="@+id/restrainView"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:text="@string/somewhat_shed_electrolytic"
            android:textColor="@color/staley_dominic_disruptive" />
    </LinearLayout>

    <CheckBox
        android:background="@color/nepotistic_televise"
        android:id="@+id/antiquaryView"
        android:layout_height="wrap_content"
        android:layout_margin="20dp"
        android:layout_width="0dp"
        android:text="@string/textual_barium_niacin_shortage"
        app:layout_constraintBottom_toTopOf="@id/pabloView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/deserveHeadroomLayout" />

    <CheckedTextView
        android:id="@+id/pabloView"
        android:layout_height="wrap_content"
        android:layout_width="0dp"
        android:text="@string/addressee_fascinate_perceive_tort"
        android:textColor="#EB7B2707"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/antiquaryView" />
</androidx.constraintlayout.widget.ConstraintLayout>
