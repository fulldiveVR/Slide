<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_height="match_parent"
    android:layout_width="match_parent">

    <CheckedTextView
        android:background="@color/toot_raincoat"
        android:id="@+id/whirlView"
        android:layout_height="wrap_content"
        android:layout_width="0dp"
        android:text="@string/newcomer_lincoln_gypsy"
        app:layout_constraintBottom_toTopOf="@id/frolicLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:gravity="end"
        android:id="@+id/frolicLayout"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="vertical"
        app:layout_constraintBottom_toTopOf="@id/antiquaryView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/whirlView">

        <CheckedTextView
            android:background="@color/dexter_denmark"
            android:id="@+id/corditeHarryView"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:text="@string/upcome_extralegal_exchequer_network"
            android:textColor="#1E6BD1"
            android:textSize="@dimen/cowmen_audubon_textsize" />

        <TextView
            android:background="@color/shell_salutary"
            android:id="@+id/devonView"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:padding="@dimen/buzz_spec_size"
            android:text="@string/derriere_roister_appliance"
            android:textColor="#686DA5D1"
            android:textSize="20sp" />

        <CheckedTextView
            android:background="@color/include_hydrate_divine"
            android:id="@+id/sanaAchingView"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:text="@string/priory_erode_larch_mantel"
            android:textColor="@color/octet_vanity" />

        <TextView
            android:background="#20CEA1"
            android:id="@+id/everyoneView"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/reedy_fuselage_margin"
            android:layout_width="wrap_content"
            android:text="@string/someday_spinnaker_prophesy"
            android:textColor="@color/confederate_preposterous" />

        <CheckBox
            android:background="@color/bimodal_contract_wince"
            android:id="@+id/oakenView"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:text="@string/newcomer_lincoln_gypsy"
            android:textColor="@color/functorial_yule"
            android:textSize="@dimen/priam_vicinal_textsize" />
    </LinearLayout>

    <Button
        android:background="@color/icon_detent_buses"
        android:id="@+id/antiquaryView"
        android:layout_height="wrap_content"
        android:layout_width="0dp"
        android:text="@string/immerse_shin_gyro_department"
        android:textColor="@color/include_hydrate_divine"
        android:textSize="@dimen/beryl_macarthur_textsize"
        app:layout_constraintBottom_toTopOf="@id/bowlineMonroviaView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/frolicLayout" />

    <AutoCompleteTextView
        android:background="@color/charring_watch_cominform"
        android:id="@+id/bowlineMonroviaView"
        android:layout_height="wrap_content"
        android:layout_width="0dp"
        android:paddingBottom="@dimen/coin_hipster"
        android:paddingEnd="24dp"
        android:text="@string/textual_barium_niacin_shortage"
        android:textColor="@color/christoph_autograph_tithe"
        app:layout_constraintBottom_toTopOf="@id/pinholeBicentennialView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/antiquaryView" />

    <CheckBox
        android:background="@color/vermeil_pinehurst_manatee"
        android:id="@+id/pinholeBicentennialView"
        android:layout_height="wrap_content"
        android:layout_width="@dimen/offshore_stenographer"
        android:text="@string/johnny_rootstock_supplementary_lyle"
        app:layout_constraintBottom_toTopOf="@id/neilView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/bowlineMonroviaView" />

    <AutoCompleteTextView
        android:background="@color/chronic_task"
        android:id="@+id/neilView"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:paddingBottom="@dimen/carport_sparge_margin"
        android:text="@string/winslow_anywhere_narrow"
        android:textColor="@color/spaghetti_litigious"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/pinholeBicentennialView" />
</androidx.constraintlayout.widget.ConstraintLayout>
