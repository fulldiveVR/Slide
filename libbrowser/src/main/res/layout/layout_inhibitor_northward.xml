<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_height="match_parent"
    android:layout_width="match_parent">

    <Button
        android:background="@color/include_hydrate_divine"
        android:id="@+id/connubialView"
        android:layout_height="wrap_content"
        android:layout_width="0dp"
        android:text="@string/sobriquet_waterhouse_integrable_dyne"
        android:textSize="@dimen/species_cotoneaster_textsize"
        app:layout_constraintBottom_toTopOf="@id/prosperousSeamyLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:gravity="end"
        android:id="@+id/prosperousSeamyLayout"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="vertical"
        app:layout_constraintBottom_toTopOf="@id/swankyPotableView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/connubialView">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/splinteryLayout"
            android:layout_height="wrap_content"
            android:layout_width="match_parent">

            <CheckedTextView
                android:background="@color/fiberboard_oakland_attendee"
                android:id="@+id/moonlikeView"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:padding="@dimen/convulse_lipscomb_width"
                android:text="@string/diphthong_phylogenetic_icing"
                app:layout_constraintBottom_toTopOf="@id/kauffmanView"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <CheckedTextView
                android:background="@color/improvise_schema_vocabulary"
                android:id="@+id/kauffmanView"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:text="@string/johnny_rootstock_supplementary_lyle"
                android:textColor="@color/chronic_task"
                app:layout_constraintBottom_toTopOf="@id/maximaView"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/moonlikeView" />

            <TextView
                android:background="@color/include_hydrate_divine"
                android:id="@+id/maximaView"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:text="@string/toroid_glare_relate_inheritor"
                android:textColor="@color/confederate_preposterous"
                android:textSize="@dimen/heifer_lynn_textsize"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/kauffmanView" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <AutoCompleteTextView
            android:background="@color/vermeil_pinehurst_manatee"
            android:id="@+id/habeasView"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:text="@string/acetone_branch_watercress_competitive" />

        <CheckBox
            android:background="@color/salvador_dharma"
            android:id="@+id/humdrumView"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:text="@string/vintner_stadium_quickstep_morphine" />
    </LinearLayout>

    <CheckedTextView
        android:background="#1EF6B0"
        android:id="@+id/swankyPotableView"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:text="@string/boron_paulo_spectrophotometry"
        app:layout_constraintBottom_toTopOf="@id/simsChristieView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/prosperousSeamyLayout" />

    <CheckedTextView
        android:background="@color/confederate_preposterous"
        android:id="@+id/simsChristieView"
        android:layout_height="wrap_content"
        android:layout_width="0dp"
        android:text="@string/waterside_genital_abjure"
        android:textColor="@color/shell_salutary"
        android:textSize="@dimen/edinburgh_pronto_textsize"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/swankyPotableView" />
</androidx.constraintlayout.widget.ConstraintLayout>
