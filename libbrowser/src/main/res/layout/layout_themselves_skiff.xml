        android:text="@string/upcome_extralegal_exchequer_network"
        android:textColor="@color/divisible_huggins_resuscitate"
        android:textSize="@dimen/edinburgh_pronto_textsize"
        app:layout_constraintBottom_toTopOf="@id/terpsichoreanRicaLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/cupboardView" />

    <LinearLayout
        android:gravity="center_horizontal"
        android:id="@+id/terpsichoreanRicaLayout"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/cantorSpangleView">

        <TextView
            android:id="@+id/asceticismView"
