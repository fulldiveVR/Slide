                app:layout_constraintBottom_toTopOf="@id/mirageView"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/adrenalCyanateLayout" />

            <TextView
                android:background="#42A78078"
                android:id="@+id/mirageView"
                android:layout_height="wrap_content"
                android:layout_width="@dimen/hacienda_culpa_margin"
                android:paddingBottom="@dimen/propos_attainder_size"
                android:paddingEnd="@dimen/expiable_obscure_width"
                android:text="@string/vermin_privacy_playa"
                android:textColor="@color/chronic_task"
                android:textSize="12sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/sublimateVidaView" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
