                    android:background="@color/statute_celery"
                    android:id="@+id/ruinView"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:padding="@dimen/ovid_extracurricular_padding"
                    android:text="@string/chou_assassin_beseech"
                    android:textColor="@color/toot_raincoat" />
            </LinearLayout>

            <AutoCompleteTextView
                android:background="@color/christoph_autograph_tithe"
                android:id="@+id/sublimateVidaView"
                android:layout_height="wrap_content"
