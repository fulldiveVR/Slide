        android:text="@string/infinitum_areaway_madcap_waterfowl"
        android:textColor="@color/lain_formosa"
        app:layout_constraintBottom_toTopOf="@id/cupboardView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/escadrilleTroubleshootView" />

    <Button
        android:background="@color/salvador_dharma"
        android:id="@+id/cupboardView"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/florence_hallucinogen_padding"
        android:layout_marginStart="@dimen/florence_hallucinogen_padding"
        android:layout_width="@dimen/pepping_compulsion_height"
