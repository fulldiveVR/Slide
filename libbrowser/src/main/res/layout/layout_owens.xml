<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_height="match_parent"
    android:layout_width="match_parent">

    <CheckedTextView
        android:background="@color/methylene_clamber_epistemology"
        android:id="@+id/lociMatinalView"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:text="@string/chou_assassin_beseech"
        android:textColor="#5A2C50"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@id/juggleNutshellLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/juggleNutshellLayout"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        app:layout_constraintBottom_toTopOf="@id/laocoonView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/lociMatinalView">

        <EditText
            android:background="@color/charring_watch_cominform"
            android:id="@+id/expungeView"
            android:layout_height="wrap_content"
            android:layout_width="0dp"
            android:text="@string/winslow_anywhere_narrow"
            android:textColor="@color/functorial_yule"
            app:layout_constraintBottom_toTopOf="@id/immoralGalileeLayout"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <LinearLayout
            android:gravity="top|center_vertical"
            android:id="@+id/immoralGalileeLayout"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="horizontal"
            app:layout_constraintBottom_toTopOf="@id/waltonSleetView"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/expungeView">

            <CheckedTextView
                android:background="@color/staley_dominic_disruptive"
                android:id="@+id/goetheAmbleView"
                android:layout_height="match_parent"
                android:layout_width="wrap_content"
                android:text="@string/prima_bragg_whet_representative" />
        </LinearLayout>

        <CheckBox
            android:background="@color/charring_watch_cominform"
            android:id="@+id/waltonSleetView"
            android:layout_height="wrap_content"
            android:layout_width="@dimen/expiable_obscure_width"
            android:padding="@dimen/carport_sparge_margin"
            android:text="@string/apprehend_eternal_glen"
            android:textColor="@color/clearwater_gaslight_nowadays"
            app:layout_constraintBottom_toTopOf="@id/cheekyView"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/immoralGalileeLayout" />

        <AutoCompleteTextView
            android:background="#2B7CE1"
            android:id="@+id/cheekyView"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/convulse_lipscomb_width"
            android:layout_width="758dp"
            android:text="@string/marshall_possum_headlight_equidistant"
            android:textColor="@color/fiberboard_oakland_attendee"
            android:textSize="@dimen/mcdonald_transect_textsize"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/waltonSleetView" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <TextView
        android:background="#909A6F8D"
        android:id="@+id/laocoonView"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/metcalf_veneer_size"
        android:layout_width="wrap_content"
        android:text="@string/vitreous_sixgun_mainstay_crackle"
        android:textColor="@color/spaghetti_litigious"
        app:layout_constraintBottom_toTopOf="@id/cabotThyratronView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/juggleNutshellLayout" />

    <CheckBox
        android:id="@+id/cabotThyratronView"
        android:layout_height="wrap_content"
        android:layout_marginBottom="22dp"
        android:layout_width="match_parent"
        android:text="@string/waterside_genital_abjure"
        app:layout_constraintBottom_toTopOf="@id/mingleView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/laocoonView" />

    <Button
        android:background="@color/spaghetti_litigious"
        android:id="@+id/mingleView"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/cursory_frayed"
        android:layout_width="wrap_content"
        android:text="@string/cyrillic_swatch_smash_more"
        android:textColor="@color/staley_dominic_disruptive"
        app:layout_constraintBottom_toTopOf="@id/expletiveView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/cabotThyratronView" />

    <CheckBox
        android:background="@color/familism_shelley"
        android:id="@+id/expletiveView"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:text="@string/textual_barium_niacin_shortage"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/mingleView" />
</androidx.constraintlayout.widget.ConstraintLayout>
