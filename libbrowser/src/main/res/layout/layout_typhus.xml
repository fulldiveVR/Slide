                <EditText
                    android:background="@color/staley_dominic_disruptive"
                    android:id="@+id/plattingView"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="@string/shuffle_jeres_wiretap"
