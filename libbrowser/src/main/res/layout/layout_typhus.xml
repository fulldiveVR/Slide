                app:layout_constraintBottom_toTopOf="@id/burroLilacLayout"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <LinearLayout
                android:gravity="top|center_vertical"
                android:id="@+id/burroLilacLayout"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintBottom_toTopOf="@id/volcanismView"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/weierstrassView">

                <EditText
                    android:background="@color/clearwater_gaslight_nowadays"
                    android:id="@+id/eddieSupremacyView"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="@string/speak_alterman_brothel_boylston"
