                app:layout_constraintTop_toBottomOf="@id/burroLilacLayout" />

            <AutoCompleteTextView
                android:background="@color/methylene_clamber_epistemology"
                android:id="@+id/preliminaryElevenfoldView"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:text="@string/farina_novosibirsk_deflate"
                android:textColor="@color/octet_vanity"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/volcanismView" />
