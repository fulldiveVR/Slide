<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_height="match_parent"
    android:layout_width="match_parent">

    <CheckedTextView
        android:background="#22E9722A"
        android:id="@+id/matriarchView"
        android:layout_height="wrap_content"
        android:layout_width="456dp"
        android:padding="48dp"
        android:text="@string/vitreous_sixgun_mainstay_crackle"
        android:textColor="@color/bimodal_contract_wince"
        app:layout_constraintBottom_toTopOf="@id/scareView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:background="@color/nepotistic_televise"
        android:id="@+id/scareView"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:padding="@dimen/producible_armenian_height"
        android:text="@string/vintner_stadium_quickstep_morphine"
        android:textColor="@color/salvador_dharma"
        android:textSize="@dimen/deathbed_verdict_textsize"
        app:layout_constraintBottom_toTopOf="@id/chalkJudiciaryView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/matriarchView" />

    <EditText
        android:background="@color/spaghetti_litigious"
        android:id="@+id/chalkJudiciaryView"
        android:layout_height="wrap_content"
        android:layout_width="0dp"
        android:text="@string/nocturne_slippery_sedulous_rooftree"
        android:textSize="@dimen/deathbed_verdict_textsize"
        app:layout_constraintBottom_toTopOf="@id/scanningCaveView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/scareView" />

    <CheckBox
        android:background="#FB90BA"
        android:id="@+id/scanningCaveView"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:text="@string/apprehend_eternal_glen"
        android:textColor="@color/improvise_schema_vocabulary"
        app:layout_constraintBottom_toTopOf="@id/leavenworthView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/chalkJudiciaryView" />

    <EditText
        android:id="@+id/leavenworthView"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:paddingBottom="@dimen/homage_delete_width"
        android:paddingEnd="@dimen/ovid_extracurricular_padding"
        android:paddingStart="@dimen/creaky_suffragette_padding"
        android:text="@string/newcomer_lincoln_gypsy"
        android:textColor="@color/judicious_herdsman"
        app:layout_constraintBottom_toTopOf="@id/opponentView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/scanningCaveView" />

    <CheckBox
        android:background="@color/confederate_preposterous"
        android:id="@+id/opponentView"
        android:layout_height="wrap_content"
        android:layout_width="@dimen/mapping_holmium_size"
        android:text="@string/isotopic_despond_rowena"
        android:textColor="@color/shell_salutary"
        app:layout_constraintBottom_toTopOf="@id/registrarLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/leavenworthView" />

    <LinearLayout
        android:gravity="top"
        android:id="@+id/registrarLayout"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="horizontal"
        app:layout_constraintBottom_toTopOf="@id/sawfishView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/opponentView">

        <CheckedTextView
            android:background="@color/chronic_task"
            android:id="@+id/iconicHandsetView"
            android:layout_height="wrap_content"
            android:layout_marginBottom="26dp"
            android:layout_marginEnd="@dimen/invocable_paramilitary"
            android:layout_marginStart="@dimen/invocable_paramilitary"
            android:layout_width="wrap_content"
            android:text="@string/roadster_biology_handpicked_tine" />

        <EditText
            android:background="@color/include_hydrate_divine"
            android:id="@+id/glottisTeratogenicView"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:padding="@dimen/ovid_extracurricular_padding"
            android:text="@string/psalter_bravado_mannitol_candlestick"
            android:textColor="@color/toot_raincoat"
            android:textSize="@dimen/lower_inhomogeneous_textsize" />

        <CheckBox
            android:background="@color/include_hydrate_divine"
            android:id="@+id/phonologyView"
            android:layout_height="match_parent"
            android:layout_width="wrap_content"
            android:text="@string/toroid_glare_relate_inheritor" />
    </LinearLayout>

    <EditText
        android:background="@color/spaghetti_litigious"
        android:id="@+id/sawfishView"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:text="@string/textual_barium_niacin_shortage"
        android:textColor="@color/insinuate_gild_incombustible"
        app:layout_constraintBottom_toTopOf="@id/compliantMulticsView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/registrarLayout" />

    <EditText
        android:id="@+id/compliantMulticsView"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:text="@string/snuffle_mozart_cashew_pyroxenite"
        android:textColor="@color/dexter_denmark"
        app:layout_constraintBottom_toTopOf="@id/polluteCovariateView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/sawfishView" />

    <EditText
        android:background="@color/icon_detent_buses"
        android:id="@+id/polluteCovariateView"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:text="@string/celesta_badinage_fulcrum_doric"
        android:textColor="#110B6E"
        android:textSize="@dimen/obligatory_propagandist_textsize"
        app:layout_constraintBottom_toTopOf="@id/duncanView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/compliantMulticsView" />

    <CheckBox
        android:background="#9E2B490A"
        android:id="@+id/duncanView"
        android:layout_height="wrap_content"
        android:layout_width="0dp"
        android:text="@string/share_bracelet_novosibirsk"
        android:textColor="@color/clearwater_gaslight_nowadays"
        app:layout_constraintBottom_toTopOf="@id/pasticheOleomargarineView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/polluteCovariateView" />

    <CheckedTextView
        android:id="@+id/pasticheOleomargarineView"
        android:layout_height="wrap_content"
        android:layout_width="@dimen/convulse_lipscomb_width"
        android:text="@string/shuffle_jeres_wiretap"
        android:textColor="#AA7DAB"
        android:textSize="@dimen/jugging_grumpy_textsize"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/duncanView" />
</androidx.constraintlayout.widget.ConstraintLayout>
