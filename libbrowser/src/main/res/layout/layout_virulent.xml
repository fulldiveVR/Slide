<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_height="match_parent"
    android:layout_width="match_parent">

    <Button
        android:background="#CB7BD3"
        android:id="@+id/swirlView"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:text="@string/strategic_sabotage_principia_briar"
        app:layout_constraintBottom_toTopOf="@id/abusableView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <AutoCompleteTextView
        android:background="#61F2D681"
        android:id="@+id/abusableView"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:text="@string/share_bracelet_novosibirsk"
        android:textColor="@color/bimodal_contract_wince"
        app:layout_constraintBottom_toTopOf="@id/succinctLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/swirlView" />

    <LinearLayout
        android:gravity="center_horizontal|start"
        android:id="@+id/succinctLayout"
        android:layout_height="wrap_content"
        android:layout_width="@dimen/reedy_fuselage_margin"
        android:orientation="vertical"
        app:layout_constraintBottom_toTopOf="@id/vampireNiagaraView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/abusableView">

        <CheckBox
            android:id="@+id/crazyPulsarView"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:text="@string/uptown_alcmena_daunt_knuckle"
            android:textColor="@color/statute_celery" />

        <EditText
            android:background="@color/icon_detent_buses"
            android:id="@+id/cheekyView"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:text="@string/dyer_pernicious_liqueur" />
    </LinearLayout>

    <EditText
        android:background="#2DD77F"
        android:id="@+id/vampireNiagaraView"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:text="@string/abed_chauvinist_liquidus"
        android:textColor="@color/christoph_autograph_tithe"
        app:layout_constraintBottom_toTopOf="@id/glazeLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/succinctLayout" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/glazeLayout"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        app:layout_constraintBottom_toTopOf="@id/commonwealFurmanView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/vampireNiagaraView">

        <EditText
            android:background="@color/spaghetti_litigious"
            android:id="@+id/styleView"
            android:layout_height="wrap_content"
            android:layout_width="@dimen/expiable_obscure_width"
            android:text="@string/trite_erasable_hines_entomb"
            android:textColor="@color/salvador_dharma"
            app:layout_constraintBottom_toTopOf="@id/hainesEmphysemaView"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:background="@color/functorial_yule"
            android:id="@+id/hainesEmphysemaView"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:text="@string/winslow_anywhere_narrow"
            android:textColor="@color/clearwater_gaslight_nowadays"
            android:textSize="@dimen/hematite_misshapen_textsize"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/styleView" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <AutoCompleteTextView
        android:background="@color/fiberboard_oakland_attendee"
        android:id="@+id/commonwealFurmanView"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:padding="@dimen/pepping_compulsion_height"
        android:text="@string/chernov_gala_fatty"
        android:textColor="@color/bimodal_contract_wince"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/glazeLayout" />
</androidx.constraintlayout.widget.ConstraintLayout>
