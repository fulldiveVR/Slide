<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_height="match_parent"
    android:layout_width="match_parent">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/moatTwiceLayout"
        android:layout_height="wrap_content"
        android:layout_width="0dp"
        app:layout_constraintBottom_toTopOf="@id/cocoonLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <Button
            android:background="@color/confederate_preposterous"
            android:id="@+id/ambulateView"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:text="@string/demiscible_rascal_surmount_moonlight"
            app:layout_constraintBottom_toTopOf="@id/marinateView"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <CheckedTextView
            android:background="@color/dexter_denmark"
            android:id="@+id/marinateView"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:padding="@dimen/madame_roister_padding"
            android:text="@string/nocturne_slippery_sedulous_rooftree"
            android:textColor="#F2A621"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/ambulateView" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/cocoonLayout"
        android:layout_height="wrap_content"
        android:layout_width="0dp"
        app:layout_constraintBottom_toTopOf="@id/versatileView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/moatTwiceLayout">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/librevilleLayout"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            app:layout_constraintBottom_toTopOf="@id/preciseView"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <CheckedTextView
                android:background="@color/wingman_vida"
                android:id="@+id/stupaTankView"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/madame_roister_padding"
                android:layout_width="wrap_content"
                android:paddingBottom="28dp"
                android:text="@string/priory_erode_larch_mantel"
                android:textColor="@color/spaghetti_litigious"
                android:textSize="@dimen/mcdonald_transect_textsize"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <TextView
            android:background="@color/familism_shelley"
            android:id="@+id/preciseView"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:text="@string/acrobatic_remediable_hopscotch"
            android:textColor="@color/staley_dominic_disruptive"
            app:layout_constraintBottom_toTopOf="@id/floridaWalthamView"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/librevilleLayout" />

        <AutoCompleteTextView
            android:background="#4D076A95"
            android:id="@+id/floridaWalthamView"
            android:layout_height="wrap_content"
            android:layout_width="0dp"
            android:text="@string/vermin_privacy_playa"
            android:textColor="@color/icon_detent_buses"
            app:layout_constraintBottom_toTopOf="@id/gertrudeView"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/preciseView" />

        <EditText
            android:background="@color/octet_vanity"
            android:id="@+id/gertrudeView"
            android:layout_height="wrap_content"
            android:layout_width="370dp"
            android:paddingEnd="@dimen/gown_colatitude"
            android:paddingTop="@dimen/creaky_suffragette_padding"
            android:text="@string/demiscible_rascal_surmount_moonlight"
            android:textColor="@color/hippie_witt_doormen"
            app:layout_constraintBottom_toTopOf="@id/transshipTineView"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/floridaWalthamView" />

        <CheckBox
            android:background="@color/oases_malfeasant_bookplate"
            android:id="@+id/transshipTineView"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:text="@string/abed_chauvinist_liquidus"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/gertrudeView" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <AutoCompleteTextView
        android:background="@color/charring_watch_cominform"
        android:id="@+id/versatileView"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:text="@string/vintner_stadium_quickstep_morphine"
        android:textColor="@color/staley_dominic_disruptive"
        android:textSize="@dimen/jugging_grumpy_textsize"
        app:layout_constraintBottom_toTopOf="@id/inmateView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/cocoonLayout" />

    <CheckedTextView
        android:background="@color/hippie_witt_doormen"
        android:id="@+id/inmateView"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:text="@string/lineup_heathkit_gasp"
        android:textColor="@color/wingman_vida"
        android:textSize="@dimen/cowmen_audubon_textsize"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/versatileView" />
</androidx.constraintlayout.widget.ConstraintLayout>
